<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>DisplayData</title>
    <link rel="stylesheet" href="css/displayDetails.css">
    <!-- CSS only -->
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
</head>
<body>
    <nav class="navbar navbar-dark bg-dark py-1">
        <a class="navbar-brand" href="#">
          <img
            src="./css/logo.png"
            width="75"
            height="50"
            alt=""
          />
          our web
        </a>
        <ul class="navbar-nav d-flex flex-row">
          <li class="nav-item active">
            <a class="nav-link p-3" href="#">Home </a>
          </li>
          <li class="nav-item active">
            <a class="nav-link p-3" href="#">about</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link p-3" href="#">content1</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link active p-3" href="#">content2</a>
          </li>
        </ul>
      </nav>
  

      <div class="container my-5">
        <h2><%= title %></h2>
          <table class="table table-dark table-striped">
            <thead>
              <tr>
                <th>Id</th>
                <th>vendorcode</th>
                <th>prodcat</th>
                <th>proddes</th>
                <th>countofprod</th>
                <th>costofprod</th>
                <th>currency</th>
                <th>prodexp</th>
              </tr>
            </thead>
            <tbody>
              <% for(var i=0;i< user.length;i++){ %>
                <tr>
                  <td><%= user[i].Id %></td>
                  <td><%= user[i].vendor_code %></td>
                  <td><%= user[i].prod_cat %></td>
                  <td><%= user[i].prod_desc %></td>
                  <td><%= user[i].count_of_prod %></td>
                  <td><%= user[i].cost_of_prod %></td>
                  <td><%= user[i].currency %></td>
                  <td><%= user[i].prod_exp %></td>
                </tr>
            <% } %> 
            </tbody>
            

          </table>
      </div>




<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
<!-- JavaScript Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
<script src="displayDetails.js"></script>
</body>
</html>